<script language="JavaScript1.2" type="text/javascript">
function Redirect(url)
{
	if(document.cookie.indexOf("RMID=") != -1)
	{
		var iBannerPos, iBannerEndPos, strBanner;
		if((iBannerPos=document.cookie.indexOf("INBANNER=")) != -1)
		{
			iBannerEndPos = document.cookie.indexOf(";", iBannerPos);
			if(iBannerEndPos != -1)
				strBanner = document.cookie.substr(iBannerPos, iBannerEndPos-iBannerPos);
			else
				strBanner = document.cookie.substr(iBannerPos);
			strBanner = strBanner.replace(/INBANNER=/ig, "");
			if(parseInt(strBanner)>=10)
			{
				window.location = url;
			}
			else
			{
				var d = new Date();
				url = escape(url);
				url.replace("+", "%2B");
				url.replace("/", "%2F");

				window.location = "http://www.rediff.com/login/inredirect.php?url=" + url + "&tz=" + d.getTimezoneOffset();
			}
		}
		else
		{
			var d = new Date();
			url = escape(url);
			url.replace("+", "%2B");
			url.replace("/", "%2F");

			window.location = "http://www.rediff.com/login/inredirect.php?url=" + url + "&tz=" + d.getTimezoneOffset();
		}
	}
	else
	{
		window.location = url;
	}
	return false;
}
</script>

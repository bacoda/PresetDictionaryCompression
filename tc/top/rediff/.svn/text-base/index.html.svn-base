<script language="JAVASCRIPT" type="text/javascript">
var ck=document.cookie;
function tr_getCk(n)
{
 var ar=n+"=";
 var al=ar.length;
 var cl=ck.length;
 var i=0;
 while(i < cl)
 {
 j=i+al;
 if(ck.substring(i,j)==ar)
 {
 e=ck.indexOf(";",j);
 if(e==-1)
 e=ck.length;
 return unescape(ck.substring(j,e));
 }
 i=ck.indexOf(" ",i)+1;
 if(i==0)
 break;
 }
 return null;
}
function tr_sCk(n,v,x,p,d) {
 var cstr;
 document.cookie = n + "=" + escape(v) +
 ((x) ? "; expires="+x : "") +
 ((p) ? "; path="+p : "") +
 ((d) ? "; domain="+d : "");
}
